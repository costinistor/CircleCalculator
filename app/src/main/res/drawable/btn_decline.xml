<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FFFFFF"/>
            <stroke
                android:width="1dp"
                android:color="#8FC1CE" />
            <corners
                android:radius="5sp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#E8F5F5" />
            <stroke
                android:width="1dp"
                android:color="#8FC1CE" />
            <corners
                android:radius="5sp" />
        </shape>
    </item>
</selector>